    ],
    "filters": "ga:eventCategory=~ownload;ga:eventLabel!~swf$;ga:pagePath!~(usps.com).*/(?i)(zip|doc).*",
    "start-index": 1,
    "max-results": 100,
    "samplingLevel": "HIGHER_PRECISION"
  },
  "meta": {
    "name": "Top Downloads (7 Days)",
    "description": "Top downloads in the last 7 days."
  },
  "data": [],
